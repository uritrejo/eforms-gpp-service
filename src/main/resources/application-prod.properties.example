# Example application-prod.properties for production environment  
# Copy this file and rename to application-prod.properties for production-specific settings

spring.application.name=eforms-gpp-service
server.port=4420

# Production CORS Configuration - more restrictive
app.cors.allowed-origins=https://yourdomain.com,https://www.yourdomain.com
app.cors.allowed-methods=GET,POST,PUT,DELETE,OPTIONS
app.cors.allowed-headers=Content-Type,Authorization,X-Requested-With

# Production API Configuration
app.api.ted-api-key=${TED_API_KEY:}
